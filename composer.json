{
    "name": "trailblazersoftware/multi-tenant",
    "description": "A Laravel 5.5+ package to help manage multi-tenancy roles and permissions.",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kolado Sidibe",
            "email": "ksidibe@yahoo.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Migrations/"
        ],
        "psr-4": {
            "Trailblazer\\MultiTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trailblazer\\MultiTenant\\MultiTenantServiceProvider"
            ]
        }
    }
}
